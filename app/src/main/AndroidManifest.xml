<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BoundServiceJava"
        tools:targetApi="31">
        <activity
            android:name=".boundservicepractice.TimeStampActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".boundservicepractice.BoundActivity"
            android:exported="false"/>



        <activity
            android:name=".MainActivity"
            android:exported="false" />

        <service
            android:name=".service.MyService"
            android:exported="false" />
        <service
            android:name=".boundservicepractice.MyServicePractice"
            android:enabled="true"
            android:exported="false" />

        <service android:name=".boundservicepractice.BoundTimeStampService"
            android:enabled="true"
            android:exported="false"/>
    </application>

</manifest>